<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        body{
            background: rgb(235, 173, 135);font-family: cursive; color: #fff;padding: 30px;
        }
        h1{
            font-size: 48px;text-transform: uppercase;letter-spacing: 2px;text-align: center;
        }
        p,li{
            font-size: 16px;text-align: center;
        }
        ul{
            list-style-type: none;padding: 0;
        }
    </style>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Contact</title>
</head>
<body>
    <%- include ('nav.ejs') %>
    <h1>CONTACT PAGE</h1>
    <p>The Id to contact is <%= id %></p>
    <p><strong> Name: </strong><%= data.name%></p>
    <p><strong> Age: </strong><%= data.age%></p>
<!-- Video 26 -->
    <h2>Hobbies</h2>
    <ul>
        <% data.hobbies.forEach(function(hobby){%>
            <li><%= hobby%></li>
        <% })%>
    </ul>
    </body>
</html>